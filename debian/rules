#! /usr/bin/make -f

%:
	dh $@

override_dh_install:
	dh_install
	install -D -o root -g root -m755 hook debian/ykfde/usr/share/initramfs-tools/hooks/ykfde
	install -D -o root -g root -m755 boot debian/ykfde/usr/share/initramfs-tools/scripts/local-top/ykfde
	install -D -o root -g root -m755 keyscript debian/ykfde/usr/share/ykfde/ykfde-keyscript
